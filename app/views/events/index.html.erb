<h1>Listing events</h1>

<table>
  <thead>
    <tr>
      <th>When</th>
      <th>What</th>
      <th>Description</th>
      <th>Where</th>
      <th>User</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.when %></td>
        <td><%= event.what %></td>
        <td><%= event.description %></td>
        <td><%= event.where %></td>


      </tr>
    <% end %>
  </tbody>
</table>

<br>


