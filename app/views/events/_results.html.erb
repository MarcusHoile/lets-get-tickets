<table class="table table-hover search-results">
	<thead id="table-head">
		<tr>
			<th>Event</th>
			<th>Venue</th>
			<th>Date</th>
		</tr>
	</thead>
	<tbody>
<% @results.each do |event| %>
		<tr class="search-result">
			<td class="event-list-name">
				<%= event["name"] %>
			</td>
			<td class="event-list-venue">
				<%= event["location"]["name"] %>
			</td>
			<td class="event-list-date">
				<%#= DateTime.strptime(event["datetime_start"], "%Y-%m-%d %I:%M:%S").strftime"%b %d %I:%M %P" %>
			</td>
			<td class="event-lat-lng hidden">
				<%= event["point"] %>
			</td>
		</tr>
	</tbody>
<% end %>